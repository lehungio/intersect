{
  "name": "intersect",
  "version": "0.1.0",
  "description": "Example code for simple collision detection",
  "homepage": "https://github.com/noonat/intersect",
  "repository": {
    "type": "git",
    "url": "https://github.com/noonat/intersect.git"
  },
  "scripts": {
    "build": "node build.js",
    "clean": "rimraf intersect.ts lib/intersect.js docs/bundle.js",
    "prepare": "npm run build",
    "test": "jest && tslint src/*.ts"
  },
  "main": "lib/intersect.js",
  "devDependencies": {
    "docco": "^0.7.0",
    "jest": "^20.0.4",
    "markdown-it": "^8.4.0",
    "rimraf": "^2.6.1",
    "ts-jest": "^20.0.11",
    "ts-loader": "^2.3.4",
    "tslint": "^5.7.0",
    "typescript": "^2.4.2",
    "webpack": "^3.5.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx"
    ],
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/test/.*\\.(ts|tsx|js|jsx)$"
  }
}
