{
  "name": "intersect",
  "version": "0.1.0",
  "description": "Example code for simple collision detection",
  "homepage": "https://github.com/noonat/intersect",
  "repository": {
    "type": "git",
    "url": "https://github.com/noonat/intersect.git"
  },
  "scripts": {
    "build": "node build.js",
    "clean": "rimraf src/intersect.ts lib/intersect.js* docs/bundle.js*",
    "prepare": "npm run build",
    "test": "jest && tslint src/*.ts"
  },
  "main": "lib/intersect.js",
  "devDependencies": {
    "docco": "^0.8.0",
    "jest": "^22.0.4",
    "markdown-it": "^8.4.0",
    "rimraf": "^2.6.2",
    "ts-jest": "^22.0.1",
    "ts-loader": "^3.2.0",
    "tslint": "^5.8.0",
    "typescript": "^2.6.2",
    "webpack": "^3.10.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx"
    ],
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/test/.*\\.(ts|tsx|js|jsx)$"
  }
}
